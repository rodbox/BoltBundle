	<div class="card-block">
		<h4 class="card-title"><a href="#" class="card-link create-item c-1 " data-type="{{ type.name }}" data-name="{{ name }}" data-ref="e{{id}}"><div class="text-muted sm">e{{ id }}.</div><i class="icon-type"></i> <strong>{{ name }}</strong> 
			<div class="text-muted sm ">{{ type.name }}</div></a></h4>
	</div>
</div>